{
  "expo": {
    "name": "YarSu",
    "slug": "yarsu",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/YarSu.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yarsu.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.yarsu.app",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/YarSu.png",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json"
      },
      "edgeToEdgeEnabled": true,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/YarSu.png"
    },
    "plugins": [
      "expo-router",
      "react-native-video",
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bc5790aa-5573-4eaf-bd30-d1690c2a00af"
      }
    }
  }
}

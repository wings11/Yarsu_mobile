{
  "cli": {
    "version": ">= 16.17.4"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "autoIncrement": true
      },
      "ios": {
        "simulator": false,
        "autoIncrement": true,
        "buildConfiguration": "Release"
      }
      // "submit": {
      //   "production": {}
      // }
    }
  }
}